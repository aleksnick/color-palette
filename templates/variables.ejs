:root {
  <% if (alphaColors) { %>
    <% Object.entries(alphaColors).forEach(([group, colors]) => { %>

  /* Alpha: <%- group %> */
<% colors.forEach((color) => { %><% if (color.var) { %>
  <%- color.var %>: <%- color.hex %>;<% } %><% }) %>
<% }) %><% } %>

  <% Object.entries(palette).forEach(([group, colors]) => { %>

  /* Palette: <%- group %> */
<% colors.forEach((color) => { %><% if (color.var) { %>
  <%- color.var %>: <%- color.hex %>;<% } %><% }) %>
<% }) %>
}
